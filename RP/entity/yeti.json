{
	"format_version": "1.20.80",
	"minecraft:client_entity": {
		"description": {
			"identifier": "myth:yeti",
			"materials": {
				"default": "entity_alphatest"
			},
			"textures": {
				"default": "textures/entity/Yeti"
			},
			"geometry": {
				"default": "geometry.yeti"
			},
			"render_controllers": [
				"controller.render.yeti"
			],
			"spawn_egg": {
				"base_color": "#7F7F7F",
				"overlay_color": "#FFFFFF"
			},
			"animations": {
				"idle": "animation.yeti.idle",
				"walk": "animation.yeti.walk",
				"run": "animation.yeti.run",
				"roar": "animation.yeti.roar",
				"attack": "animation.yeti.attack"
			},
			"scripts": {
				"animate": [
					{
						"idle": "query.modified_move_speed<=0.01"
					},
					{
						"walk": "query.modified_move_speed>=0.1&&query.modified_move_speed<0.5"
					},
					{
						"run": "query.modified_move_speed>=0.5"
					}
				]
			}
		}
	}
}